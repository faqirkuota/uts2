<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ReadActivity">
    <TextView
            android:id="@+id/Judul"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Data Covid"
            android:layout_alignParentRight="true"
            android:layout_alignParentLeft="true"
            android:textAlignment="center"
            android:fontFamily="sans-serif-condensed"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="40sp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="36dp" app:layout_constraintHorizontal_bias="0.065"/>
    <TextView
            android:id="@+id/Judul2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Indonesia"
            android:layout_alignParentRight="true"
            android:layout_alignParentLeft="true"
            android:textAlignment="center"
            android:fontFamily="sans-serif-condensed"
            android:textColor="#7d0000"
            android:textStyle="bold"
            android:textSize="30sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.053" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="72dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="411dp"
            android:layout_height="539dp"
            android:orientation="vertical"
            app:layout_constraintTop_toBottomOf="@+id/Judul2" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="20dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:background="@drawable/home_layout">
        <TextView
                android:id="@+id/txtNamaKecamatan"
                android:layout_width="329dp"
                android:layout_height="41dp"
                android:hint="Data {namaKecamatan}"
                android:layout_alignParentRight="true"
                android:layout_alignParentLeft="true"
                android:textAlignment="textStart"
                android:fontFamily="sans-serif-condensed"
                android:textColor="#7d0000"
                android:textStyle="normal"
                android:textSize="25sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.181" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="16dp"/>
        <TextView
                android:id="@+id/txtPositif"
                android:layout_width="369dp"
                android:layout_height="37dp"
                android:hint="Jumlah Positif"
                android:fontFamily="sans-serif-condensed"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:editable="false"
                android:background="@drawable/home_layout"
                android:padding="4dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.333"
                app:layout_constraintTop_toBottomOf="@+id/txtNamaKecamatan" android:layout_marginTop="20dp"/>
        <TextView
                android:id="@+id/txtSembuh"
                android:layout_width="369dp"
                android:layout_height="37dp"
                android:hint="Jumlah Sembuh"
                android:fontFamily="sans-serif-condensed"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:editable="false"
                android:background="@drawable/home_layout"
                android:padding="4dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.291"
                app:layout_constraintTop_toBottomOf="@+id/txtPositif" android:layout_marginTop="20dp"/>
        <TextView
                android:id="@+id/txtKematian"
                android:layout_width="369dp"
                android:layout_height="37dp"
                android:hint="Jumlah Kematian"
                android:fontFamily="sans-serif-condensed"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:editable="false"
                android:background="@drawable/home_layout"
                android:padding="4dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.291"
                app:layout_constraintTop_toBottomOf="@+id/txtSembuh" android:layout_marginTop="20dp"/>
        <Button
                android:text="Update"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/btnUpdate"
                app:layout_constraintTop_toBottomOf="@+id/txtKematian" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="20dp"
                app:layout_constraintHorizontal_bias="0.049"/>
        <Button
                android:text="Delete"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/btnDelete"
                app:layout_constraintTop_toBottomOf="@+id/txtKematian" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="20dp"
                app:layout_constraintHorizontal_bias="0.362"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>